<metadata>
    <!--
        not surfacing/generating MCWs 
        Compose does work for kotlin only via Annotations and kotlin defined UI
        it does not make sense for C# until parsing of kotlin and transpiling to c# is ready (if ever)
    -->
    <remove-node path="path=/api/package" />

    <attr 
        path="/api/package[@name='androidx.compose.ui.graphics']" 
        name="managedName"
        >
        AndroidX.Compose.UI.Graphics
    </attr>
    <attr 
        path="/api/package[@name='androidx.compose.ui.graphics.colorspace']" 
        name="managedName"
        >
        AndroidX.Compose.UI.Graphics.Colorspace
    </attr>
    <attr 
        path="/api/package[@name='androidx.compose.ui.graphics.drawscope']" 
        name="managedName"
        >
        AndroidX.Compose.UI.Graphics.Drawscope
    </attr>
    <attr 
        path="/api/package[@name='androidx.compose.ui.graphics.painter']" 
        name="managedName"
        >
        AndroidX.Compose.UI.Graphics.Painter
    </attr>
    <attr 
        path="/api/package[@name='androidx.compose.ui.graphics.vector']" 
        name="managedName"
        >
        AndroidX.Compose.UI.Graphics.Vector
    </attr>

</metadata>